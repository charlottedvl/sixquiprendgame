<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.isep.sixquiprendgame.FinalScreenController">
   <children>
      <BorderPane prefHeight="409.0" prefWidth="600.0">
         <center>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="playerColumn" prefWidth="75.0" text="Joueurs" />
                <TableColumn fx:id="pointsColumn" prefWidth="153.0" sortType="DESCENDING" text="Points" />
                  <TableColumn fx:id="rankingColumn" prefWidth="246.0" text="Classement" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
         <top>
            <Text fx:id="congratText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" BorderPane.alignment="CENTER" />
         </top></BorderPane>
   </children>
</VBox>
